{
  "name": "svelte-canvas",
  "version": "1.0.2",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "build-web": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dnass/svelte-canvas.git"
  },
  "keywords": [
    "svelte",
    "canvas"
  ],
  "license": "ISC",
  "dependencies": {
    "hit-canvas": "^0.1.0",
    "svelte": "^5.0.0-next.12"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.27.6",
    "@sveltejs/package": "^2.2.2",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "d3-delaunay": "^6.0.2",
    "d3-geo": "^3.0.1",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "geojson": "^0.5.0",
    "mdsvex": "^0.11.0",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "svelte-check": "^3.4.3",
    "svelte-highlight": "^7.2.1",
    "svelte-preprocess": "^5.0.3",
    "topojson-client": "^3.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.2"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "typesVersions": {
    ">4.0": {
      "components/Canvas.svelte": [
        "./dist/components/Canvas.svelte.d.ts"
      ],
      "components/Layer.svelte": [
        "./dist/components/Layer.svelte.d.ts"
      ],
      "index": [
        "./dist/index.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ],
      "util/LayerManager": [
        "./dist/util/LayerManager.d.ts"
      ]
    }
  }
}
