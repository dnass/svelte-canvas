{
  "name": "svelte-canvas",
  "version": "0.9.3",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "build-web": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dnass/svelte-canvas.git"
  },
  "keywords": [
    "svelte",
    "canvas"
  ],
  "license": "ISC",
  "dependencies": {
    "svelte": "^3.55.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.15.1",
    "@sveltejs/package": "^2.0.2",
    "@types/d3-array": "^3.0.3",
    "@types/d3-delaunay": "^6.0.1",
    "@types/d3-geo": "^3.0.2",
    "@types/d3-interpolate": "^3.0.1",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/topojson-client": "^3.1.1",
    "@types/topojson-specification": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "d3-delaunay": "^6.0.2",
    "d3-geo": "^3.0.1",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "geojson": "^0.5.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.0",
    "topojson-client": "^3.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^4.0.3"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "typesVersions": {
    ">4.0": {
      "components/Canvas.svelte": [
        "./dist/components/Canvas.svelte.d.ts"
      ],
      "components/Layer.svelte": [
        "./dist/components/Layer.svelte.d.ts"
      ],
      "components/layerEvent": [
        "./dist/components/layerEvent.d.ts"
      ],
      "components/render": [
        "./dist/components/render.d.ts"
      ],
      "components/timer": [
        "./dist/components/timer.d.ts"
      ],
      "index": [
        "./dist/index.d.ts"
      ],
      "util/LayerManager": [
        "./dist/util/LayerManager.d.ts"
      ],
      "util/color": [
        "./dist/util/color.d.ts"
      ],
      "util/contextProxy": [
        "./dist/util/contextProxy.d.ts"
      ]
    }
  }
}
